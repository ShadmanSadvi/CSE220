    "def removeElement(source, index, size):\n",
    "    if index < size:\n",
    "        for x in range(index, len(source)-1):\n",
    "            source[x] = source[x+1]\n",
    "            source[len(source)-1] = 0\n",
    "   \n",
    "source=[10,20,30,40,50,0,0]\n",
    "removeElement(source,5,2)\n",
    "\n",
    "print(source)\n",
    "\n",
    "    "
   ]
