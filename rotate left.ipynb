    "def rotateLeft(source, k):\n",
    "    for x in range(k):\n",
    "        x = source[0]\n",
    "        for i in range(1, len(source)):\n",
    "            source[i-1] = source[i]\n",
    "        source[len(source)-1] = x\n",
    "      \n",
    "source=[10,20,30,40,50,60]\n",
    "rotateLeft(source,3)\n",
    "print(source)"
   ]
  }
